{
  "$schema": "https://schemas.wp.org/wp/6.5/theme.json",
  "settings": {
    "appearanceTools": true,
    "layout": {
      "contentSize": "620px",
      "wideSize": "1000px"
    },
    "spacing": {
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
          "name": "System Font",
          "slug": "system-font"
        }
      ]
    },
    "useRootPaddingAwareAlignments": true,
    "color": {
      "palette": [
        {
          "name": "Cyan",
          "slug": "cyan",
          "color": "#00eeff"
        },
        {
          "name": "Green",
          "slug": "green",
          "color": "#91ff00"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#ffffff"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#000000"
        },
        {
          "name": "Orange",
          "slug": "orange",
          "color": "#fb2e00"
        },
        {
          "name": "Pink",
          "slug": "pink",
          "color": "#ff00c7"
        },
        {
          "name": "Dark Purple",
          "slug": "dark-purple",
          "color": "#650d94"
        }
      ]
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header"
    },
    {
      "area": "footer",
      "name": "footer"
    }
  ],
  "version": 2
}
